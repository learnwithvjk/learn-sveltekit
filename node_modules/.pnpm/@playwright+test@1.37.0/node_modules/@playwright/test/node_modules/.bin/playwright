#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vijayakumarboopathy/Downloads/web-apps/sveltekit-project/node_modules/.pnpm/@playwright+test@1.37.0/node_modules/@playwright/test/node_modules:/Users/vijayakumarboopathy/Downloads/web-apps/sveltekit-project/node_modules/.pnpm/@playwright+test@1.37.0/node_modules/@playwright/node_modules:/Users/vijayakumarboopathy/Downloads/web-apps/sveltekit-project/node_modules/.pnpm/@playwright+test@1.37.0/node_modules:/Users/vijayakumarboopathy/Downloads/web-apps/sveltekit-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vijayakumarboopathy/Downloads/web-apps/sveltekit-project/node_modules/.pnpm/@playwright+test@1.37.0/node_modules/@playwright/test/node_modules:/Users/vijayakumarboopathy/Downloads/web-apps/sveltekit-project/node_modules/.pnpm/@playwright+test@1.37.0/node_modules/@playwright/node_modules:/Users/vijayakumarboopathy/Downloads/web-apps/sveltekit-project/node_modules/.pnpm/@playwright+test@1.37.0/node_modules:/Users/vijayakumarboopathy/Downloads/web-apps/sveltekit-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
